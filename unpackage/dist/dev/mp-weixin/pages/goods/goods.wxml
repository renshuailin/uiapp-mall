<view><view class="header"><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['select',[index]]]]]}}" class="{{['target',(item.selected)?'on':'']}}" bindtap="__e">{{''+item.text+''}}</view></block></view><view class="place"></view><view class="goods"><view class="goods-list"><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="goods_id"><view data-event-opts="{{[['tap',[['goods',['$0'],[[['goodsList','goods_id',item.goods_id]]]]]]]}}" class="product" bindtap="__e"><image src="{{item.img}}" mode="widthFix"></image><view class="name">{{item.name}}</view><view class="info"><view class="price"><text>{{"￥"+item.price}}</text></view><view class="collect">{{''+item.slogan+'付款'}}</view></view></view></block></view><view class="loading">{{loadText}}</view></view></view>