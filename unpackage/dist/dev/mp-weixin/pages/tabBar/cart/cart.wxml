<view><view class="list"><block wx:if="{{list.length==0}}"><view class="empty">购物车空空如也</view></block><block wx:else><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="row"><view data-event-opts="{{[['tap',[['del',['$0'],[[['list','',index]]]]]]]}}" class="menu" bindtap="__e"><view class="icon iconfont"></view></view><view data-event-opts="{{[['touchstart',[['touchstart',[index,'$event']]]],['touchmove',[['touchmove',[index,'$event']]]],['touchend',[['touchend',[index,'$event']]]]]}}" class="{{['product',theindex==index?'open':oldindex==index?'close':'']}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e"><view data-event-opts="{{[['tap',[['checkbox',['$0'],[[['list','',index]]]]]]]}}" class="container" bindtap="__e"><view class="checkbox"><view class="{{[(item.selected)?'on':'']}}"></view></view></view><view data-event-opts="{{[['tap',[['goodsInfo',['$0'],[[['list','',index]]]]]]]}}" class="goods" bindtap="__e"><view class="img"><image src="{{item.img}}" mode></image></view><view class="info"><view class=" title">{{item.name}}</view><view class="spec">{{item.spec}}</view><view class="number" style="{{'width:'+('width')+';'}}"><view class="price">{{"￥"+item.price}}</view><count class="count" vue-id="{{'63df024f-1-'+index}}" itemInfo="{{item}}" data-event-opts="{{[['^chang',[['sum']]]]}}" bind:chang="__e" bind:__l="__l"></count></view></view></view></view></view></block></block></view><view class="footer" style="{{'bottom:'+(footer)+';'}}"><view data-event-opts="{{[['tap',[['cancelAll',['$event']]]]]}}" class="container" bindtap="__e"><view class="checkbox"><view class="{{[(this.list.length>0?all:false)?'on':'']}}"></view></view><view class="text">全选</view></view><block wx:if="{{select.length>0}}"><view data-event-opts="{{[['tap',[['delAll',['$event']]]]]}}" class="del" bindtap="__e">删除</view></block><view class="total"><view class="sum">合计:<view class="money">{{"￥"+price}}</view></view><view data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="btn" bindtap="__e">{{"结算("+select.length+')'}}</view></view></view></view>